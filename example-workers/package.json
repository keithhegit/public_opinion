{
  "name": "bettafish-workers",
  "version": "1.0.0",
  "description": "BettaFish Cloudflare Workers API",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --env production",
    "deploy:dev": "wrangler deploy --env development",
    "db:migrate": "wrangler d1 execute bettafish-db --file=./schema.sql",
    "db:migrate:dev": "wrangler d1 execute bettafish-db-dev --file=./schema.sql"
  },
  "dependencies": {
    "hono": "^3.11.0",
    "jose": "^5.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.19.0"
  }
}

